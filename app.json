{
  "expo": {
    "name": "aetcanvas",
    "slug": "aet-canvas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_dark.png",
    "backgroundColor": "#181818",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon_dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#181818"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "backgroundColor": "#181818",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.aet.aetcanvas",
      "entitlements": {},
      "developmentTeam": "C737B74TPA"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_dark.png",
        "backgroundColor": "#181818"
      },
      "usesCleartextTraffic": true,
      "package": "com.aet.aetcanvas"
    },
    "web": {
      "favicon": "./assets/icon_dark.png"
    },
    "plugins": [
      "expo-font",
      "./plugins/disablePushNotifications"
    ],
    "extra": {
      "eas": {
        "projectId": "1975470f-e37c-4ab1-8f49-390474a66964"
      }
    }
  }
}
